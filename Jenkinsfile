#!groovy
pipeline{
	agent none
	stages{
		stage('code fetch'){
			steps{
				echo "Code Fetching"
			}
		}
		stage('Build artifacts'){
			steps{
				echo "Building artifacts"
			}
		}
		stage('Push Artifacts'){
			steps{
				echo "Pushing Artifacts"
			}
		}
		stage('Code Deploy'){
			steps{
				echo "Deploying the code"
			}
		}
	}
}
